package JavaPractice_Assignements;

import java.util.Scanner;

public class ReadIntegerfromUserLoop {

	public static void main(String[] args) {
		Scanner obj1 = new Scanner(System.in);
		System.out.println("Enter the numbers to find the Avg using Loop");
		int a = obj1.nextInt();

		for (int i = 0; i <= 255; i++) {
			if ((char) i == 'q') {
				System.out.println("Enter the q to exit");
			}

			else {
				a = (a + a);
				int b = a / i;
			}
		}

	}
}
