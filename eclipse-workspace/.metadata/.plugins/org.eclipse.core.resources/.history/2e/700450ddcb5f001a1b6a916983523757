package JavaPractice_Assignements;

import java.util.Scanner;

public class AlphabetFinder {

	public static void main(String[] args) {
		
		Scanner obj1=new Scanner(System.in);
		
		System.out.println("Enter the character in upper case or Lower case : ");
		
		String userinput=obj1.nextLine();
		
		if(userinput.equalsIgnoreCase(String.valueOf(65-92))) {
			System.out.println("you entered is upper case letter");
		}

	}

}
